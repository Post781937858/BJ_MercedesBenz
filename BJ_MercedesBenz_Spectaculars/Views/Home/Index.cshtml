    <script type="text/javascript">

        function ShuttleMap(data) {

            var ShuttleChart = echarts.init(document.getElementById('main'));

            var option = {
                title: {
                    text: '穿梭车库'
                },
                tooltip: {
                    trigger: 'item',
                    formatter: "{a} <br/>{b}: {c} ({d}%)"
                },
                legend: {
                    orient: 'vertical',
                    x: 'right',
                    data: ['满库率', '空库率'],
                },
                series: [
                    {
                        name: '占比',
                        type: 'pie',
                        radius: ['32%', '70%'],
                        avoidLabelOverlap: false,
                        label: {
                            normal: {
                                show: true,
                                position: 'inner',
                                formatter: '{b}({d}%)'//多值的嵌套
                            }
                        },
                        labelLine: {
                            normal: {
                                show: false
                            }
                        },
                        data: data,
                        itemStyle: {
                            emphasis: {
                                shadowBlur: 10,
                                shadowOffsetX: 0,
                                shadowColor: 'rgba(0, 0, 0, 0.5)'
                            },
                            normal: {
                                color: function (params) {
                                    //自定义颜色
                                    var colorList = [
                                        '#f1c621', '#cbc8d3',
                                    ];
                                    return colorList[params.dataIndex]
                                }
                            }
                        }
                    }
                ]
            };
            ShuttleChart.setOption(option);
        }

        function atticMap(data) {


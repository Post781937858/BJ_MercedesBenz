                            },
                            normal: {
                                color: function (params) {
                                    //自定义颜色
                                    var colorList = [
                                        '#92c21e', '#cbc8d3',
                                    ];
                                    return colorList[params.dataIndex]
                                }
                            }
                        }
                    }
                ]
            };
            atticChart.setOption(option1);
        }

        function HeavyMap(data) {

